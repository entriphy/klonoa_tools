{
  "name": "triph_tools",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "entriphy",
    "url": "https://github.com/entriphy"
  },
  "scripts": {
    "dev": "npm run prepare && vite",
    "build": "npm run prepare && vite build",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "preview": "vite preview",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui/banner": "^7.0.0-beta.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/checkbox": "^7.0.0-beta.0",
    "@smui/drawer": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/paper": "^7.0.0-beta.0",
    "@smui/snackbar": "^7.0.0-beta.0",
    "@smui/top-app-bar": "^7.0.0-beta.0",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "node-sass": "^8.0.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.52.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.9.3",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "sass": "^1.56.1",
    "svelte-material-ui": "^7.0.0-beta.0",
    "svelte-scrollto-element": "^0.7.0"
  }
